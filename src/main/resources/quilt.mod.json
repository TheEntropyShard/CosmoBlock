{
    "schema_version": 1,
    "quilt_loader": {
        "group": "${mod_group}",
        "id": "${mod_id}",
        "version": "${mod_version}",
        "intermediate_mappings": "net.fabricmc:intermediary",
        "metadata": {
            "name": "${mod_name}",
            "description": "Play CosmoBlock!",
            "contributors": {
                "TheEntropyShard": "Owner"
            },
            "contact": {
                "homepage": "https://github.com/TheEntropyShard/CosmoBlock",
                "issues": "https://github.com/TheEntropyShard/CosmoBlock/issues",
                "sources": "https://github.com/TheEntropyShard/CosmoBlock"
            },
            "icon": "assets/cosmoblock/icon.png"
        },
        "entrypoints": {
            "init": "me.theentropyshard.cosmoblock.CosmoBlockMod"
        },
        "depends": [
            {
                "id": "cosmicquilt",
                "versions": "*"
            },
            {
                "id": "cosmicreach",
                "versions": "*"
            }
        ]
    },
    "access_widener": "cosmoblock.accesswidener",
    "mixin": "cosmoblock.mixins.json"
}
